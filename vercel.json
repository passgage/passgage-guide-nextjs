{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/ios.html",
      "destination": "/ios",
      "permanent": true
    },
    {
      "source": "/android.html",
      "destination": "/android",
      "permanent": true
    },
    {
      "source": "/access-tag.html",
      "destination": "/access-tag",
      "permanent": true
    }
  ]
}
